            "Conditions": []
          },
          {
            "ID": "Daily09",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_001",
              "SourceString": "早上问候",
              "LocalizedString": "早上问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC014_greet_001",
              "SourceString": "早上好，新的一天开始了！",
              "LocalizedString": "早上好，新的一天开始了！"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC014/Play_Vo_Char_NPC014_greet_001.Play_Vo_Char_NPC014_greet_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily10",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_002",
              "SourceString": "中午问候",
              "LocalizedString": "中午问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC014_greet_002",
              "SourceString": "中午好，想好午餐吃什么了吗？要不要听听我的推荐？",
              "LocalizedString": "中午好，想好午餐吃什么了吗？要不要听听我的推荐？"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC014/Play_Vo_Char_NPC014_greet_002.Play_Vo_Char_NPC014_greet_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily11",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_003",
              "SourceString": "傍晚问候",
              "LocalizedString": "傍晚问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC014_greet_003",
              "SourceString": "晚上好，我正打算出去散步，要一起吗？",
              "LocalizedString": "晚上好，我正打算出去散步，要一起吗？"
            },
            "AudioEvent": {
