          {
            "ID": "Daily07",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_003",
              "SourceString": "寒冷",
              "LocalizedString": "寒冷"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC014_weather_003",
              "SourceString": "好冷好冷好冷这个时候最适合吃火锅了吧！要超级加倍辣！",
              "LocalizedString": "好冷好冷好冷这个时候最适合吃火锅了吧！要超级加倍辣！"
