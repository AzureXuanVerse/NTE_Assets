          {
            "ID": "Daily09",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_001",
              "SourceString": "早上问候",
              "LocalizedString": "早上问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC003_greet_001",
              "SourceString": "头发和衣服都没整理好，这么想见到我？",
              "LocalizedString": "头发和衣服都没整理好，这么想见到我？"
            },
