            "Conditions": []
          },
          {
            "ID": "Daily11",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_003",
              "SourceString": "傍晚问候",
              "LocalizedString": "傍晚问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC003_greet_003",
              "SourceString": "既然天要黑了，不如去些适合晚上去的地方？",
              "LocalizedString": "既然天要黑了，不如去些适合晚上去的地方？"
