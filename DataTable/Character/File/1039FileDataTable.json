          {
            "ID": "Daily02",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC003_001",
              "SourceString": "闲聊·工作",
              "LocalizedString": "闲聊·工作"
