            "Conditions": []
          },
          {
            "ID": "Daily14",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "vexation_001",
              "SourceString": "烦恼",
              "LocalizedString": "烦恼"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC003_vexation_001",
              "SourceString": "要是没有翳那只看门狗每次死盯着我，我可是会在任务里玩得更尽兴呢。",
              "LocalizedString": "要是没有翳那只看门狗每次死盯着我，我可是会在任务里玩得更尽兴呢。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC003/Play_Vo_Char_NPC003_vexation_001.Play_Vo_Char_NPC003_vexation_001",
              "SubPathString": ""
