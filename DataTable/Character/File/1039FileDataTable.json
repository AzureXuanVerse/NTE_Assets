            "Conditions": []
          },
          {
            "ID": "Daily12",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_004",
              "SourceString": "晚安",
              "LocalizedString": "晚安"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC003_greet_004",
              "SourceString": "夜晚才刚刚开始，这就要说晚安了？",
              "LocalizedString": "夜晚才刚刚开始，这就要说晚安了？"
