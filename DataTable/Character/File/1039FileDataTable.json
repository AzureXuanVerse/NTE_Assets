          {
            "ID": "Daily03",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC003_002",
              "SourceString": "闲聊·路人",
              "LocalizedString": "闲聊·路人"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC003_chat_002",
